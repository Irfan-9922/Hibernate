package com.model;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;

/**
 * NAME:shaikh irfan
 *
 * TIME:11:26:39 pm
 *DATE:08-Sep-2017
 *TAGS:
 */
@Entity
public class girlfriends {
	@Id @GeneratedValue
	private int phno;
	private String name;
	private int age;
	public int getPhno() {
		return phno;
	}
	public void setPhno(int phno) {
		this.phno = phno;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "girlfriends [phno=" + phno + ", name=" + name + ", age=" + age + "]";
	}

}
